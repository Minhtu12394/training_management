<tr>
  <td><%= subject.subject_name %></td>
  <td class="center">
    <%= link_to (t "admin.subjects.new.show"), admin_subject_path(subject), class: "btn btn-primary" %>
    <%= link_to (t "admin.subjects.new.edit_subject"), edit_admin_subject_path(subject), class: "btn btn-info" %>
    <%= link_to (t "admin.subjects.new.destroy_subject"), admin_subject_path(subject),
      class: "btn btn-danger", method: :delete, data: { confirm: t(".sure")} %>
  </td>
</tr>
